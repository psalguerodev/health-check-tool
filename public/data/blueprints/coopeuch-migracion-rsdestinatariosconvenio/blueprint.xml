<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:camelcxf="http://camel.apache.org/schema/blueprint/cxf"
    xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.1.0"
    xmlns:http="http://cxf.apache.org/transports/http/configuration"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 https://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd                                 http://camel.apache.org/schema/blueprint http://camel.apache.org/schema/blueprint/camel-blueprint.xsd">
    <cm:property-placeholder id="propertyGlobal" persistent-id="global"
        placeholder-prefix="{[" placeholder-suffix="]}" update-strategy="reload"/>
    <cm:property-placeholder id="propertyLocal"
        persistent-id="wsdestinatarioconvenio" placeholder-prefix="[{"
        placeholder-suffix="}]" update-strategy="reload"/>
    <bean class="org.apache.activemq.camel.component.ActiveMQComponent" id="beanAMQ">
        <property name="brokerURL" value="{[amq.broker.url]}"/>
        <property name="userName" value="{[amq.username]}"/>
        <property name="password" value="{[amq.password]}"/>
        <property name="usePooledConnection" value="true"/>
    </bean>
    <bean class="org.apache.commons.dbcp.BasicDataSource"
        destroy-method="close" id="dataSourceSqlServer">
        <property name="driverClassName" value="net.sourceforge.jtds.jdbc.Driver"/>
        <property name="url" value="[{jdbc.sqlserver.url.destinatario}]/Notificacion;charset=iso_1"/>
        <property name="username" value="[{jdbc.sqlserver.username.destinatario}]"/>
        <property name="password" value="[{jdbc.sqlserver.password.destinatario}]"/>
        <property name="timeBetweenEvictionRunsMillis" value="[{jdbc.sqlserver.timeBetweenEvictionRunsMillis.destinatario}]"/>
        <property name="numTestsPerEvictionRun" value="[{jdbc.sqlserver.numTestsPerEvictionRun.destinatario}]"/>
        <property name="minEvictableIdleTimeMillis" value="[{jdbc.sqlserver.minEvictableIdleTimeMillis.destinatario}]"/>
        <property name="maxActive" value="[{jdbc.sqlserver.maxActive.destinatario}]"/>
    </bean>
    <bean class="org.apache.commons.dbcp.BasicDataSource"
        destroy-method="close" id="dataSourcePortalConvenio">
        <property name="driverClassName" value="net.sourceforge.jtds.jdbc.Driver"/>
        <property name="url" value="[{jdbc.sqlserver.url.portal.convenio}]/PortalConvenio;charset=iso_1"/>
        <property name="username" value="[{jdbc.sqlserver.username.portal.convenio}]"/>
        <property name="password" value="[{jdbc.sqlserver.password.portal.convenio}]"/>
        <property name="timeBetweenEvictionRunsMillis" value="[{jdbc.sqlserver.timeBetweenEvictionRunsMillis.portal.convenio}]"/>
        <property name="numTestsPerEvictionRun" value="[{jdbc.sqlserver.numTestsPerEvictionRun.portal.convenio}]"/>
        <property name="minEvictableIdleTimeMillis" value="[{jdbc.sqlserver.minEvictableIdleTimeMillis.portal.convenio}]"/>
        <property name="maxActive" value="[{jdbc.sqlserver.maxActive.portal.convenio}]"/>
    </bean>
    <bean class="cl.coopeuch.integracion.util.RouteFacade" id="beanRouteFacade"/>
    <camelcxf:cxfEndpoint address="/WSDestinatariosConvenio"
        id="endpointWSDestinatariosConvenio"
        serviceClass="cl.coopeuch.integracion.utilidad.wsdl.wsdestinatario.convenio.WSDestinatariosConvenioSOAPPortType" wsdlURL="etc/wsdl/WSDestinatariosConvenio.wsdl">
        <camelcxf:properties>
            <entry key="schema-validation-enabled" value="true"/>
        </camelcxf:properties>
    </camelcxf:cxfEndpoint>
    <camelcxf:cxfEndpoint
        address="[{wsdestinatario.servicio.wssucursal.endpoint}]"
        id="endpointWSSucursal" loggingFeatureEnabled="true"
        serviceClass="cl.coopeuch.integracion.utilidad.wsdl.wssucursal.WSSucursalSOAPPortType" wsdlURL="etc/wsdl/WSSucursal.wsdl">
        <camelcxf:properties>
            <entry key="schema-validation-enabled" value="true"/>
        </camelcxf:properties>
    </camelcxf:cxfEndpoint>
    <camelcxf:cxfEndpoint
        address="[{wsdestinatario.servicio.wsejecutivo.endpoint}]"
        id="endpointWSEjecutivo" loggingFeatureEnabled="true"
        serviceClass="cl.coopeuch.integracion.utilidad.wsdl.wsejecutivo.WSEjecutivoPortType" wsdlURL="etc/wsdl/WSEjecutivo.wsdl"/>
    <camelContext id="wsdestinatarios-convenio" xmlns="http://camel.apache.org/schema/blueprint">
        <route id="cxf-inicio">
            <from id="inicioFromServicio" uri="cxf:bean:endpointWSDestinatariosConvenio"/>
            <doTry id="inicioTry">
                <setProperty id="inicioSetRequest" propertyName="requestEntrada">
                    <simple>${body}</simple>
                </setProperty>
                <setProperty id="bodyEntrada" propertyName="bodyEntrada">
                    <simple>$simple{body.get(0)}</simple>
                </setProperty>
                <setProperty id="inicioSetTipoRequest" propertyName="tipoRequest">
                    <simple>$simple{body.get(0).class.getSimpleName()}</simple>
                </setProperty>
                <choice id="inicioChoiceOperacion">
                    <when id="WhenConsultarVBEmpleadorRequest">
                        <simple>${header.operationName} == 'consultarVBEmpleador'</simple>
                        <to id="obtenerDestinatario-To" uri="direct:consultarVB"/>
                    </when>
                    <when id="WhenOperacionWSDestinatario">
                        <simple>${header.operationName} == 'consultarVBResolucion'</simple>
                        <to id="obtenerDestinatario-To" uri="direct:consultarVB"/>
                    </when>
                    <otherwise id="inicioOtherwise">
                        <throwException
                            exceptionType="java.lang.Exception"
                            id="throwExceptionOperacionInvalida" message="La operacion solicitada es invalida"/>
                    </otherwise>
                </choice>
                <doCatch id="inicioDoCatch">
                    <exception>java.lang.Exception</exception>
                    <to id="toInicioExcepcion" uri="direct:excepcion"/>
                </doCatch>
            </doTry>
        </route>
        <route id="consultarVB">
            <from id="consultarVB-From" uri="direct:consultarVB"/>
            <doTry id="consultarVB-DoTry">
                <setBody id="consultarVB-SetBody">
                    <simple>$simple{body.get(0)}</simple>
                </setBody>
                <setProperty id="consultarVB-CanalLllanada" propertyName="CanalLlamada">
                    <simple>$simple{body.canalLlamada}</simple>
                </setProperty>
                <setProperty id="consultarVB-IdNotificacion" propertyName="IdNotificacion">
                    <simple>$simple{body.idNotificacion}</simple>
                </setProperty>
                <bean id="consultarVB-Bean"
                    method="obtenerValoresEntrada" ref="beanRouteFacade"/>
                <to id="OrquestadorConsultarVB-consultarVB-To" uri="direct:OrquestadorConsultarVB"/>
                <doCatch id="consultarVB-DoCatch">
                    <exception>java.lang.Exception</exception>
                    <to id="toCrearExcepcion" uri="direct:excepcion"/>
                </doCatch>
            </doTry>
        </route>
        <route id="Orquestador-consultarVB">
            <from id="consultarVB-From" uri="direct:OrquestadorConsultarVB"/>
            <doTry id="Orquestador-consultarVB-DoTry">
                <to id="validarCamposEntrada-To" uri="direct:validarCamposEntrada"/>
                <to id="pa_consultarHabilitadosSolicitud-To" uri="direct:pa_consultarHabilitadosSolicitud"/>
                <to id="ObtenerDatosECC-To" uri="direct:ObtenerDatosECC"/>
                <to id="ObtenerDatosPersonalSucursal-To" uri="direct:ObtenerDatosPersonalSucursal"/>
                <removeHeaders id="Orquestador-RemoveHeaders" pattern="*"/>
                <setProperty id="Orquestador-CONTENT_TYPE" propertyName="Exchange.CONTENT_TYPE">
                    <constant>text/xml;charset=utf-8</constant>
                </setProperty>
                <setProperty id="Orquestador-HTTP_CHARACTER_ENCODING" propertyName="Exchange.HTTP_CHARACTER_ENCODING">
                    <constant>utf-8</constant>
                </setProperty>
                <setProperty id="Orquestador-CONTENT_ENCODING" propertyName="Exchange.CONTENT_ENCODING">
                    <constant>utf-8</constant>
                </setProperty>
                <setProperty id="Orquestador-CHARSET_NAME" propertyName="Exchange.CHARSET_NAME">
                    <constant>utf-8</constant>
                </setProperty>
                <doCatch id="Orquestador-consultarVB-DoCatch">
                    <exception>java.lang.Exception</exception>
                    <to id="Orquestador-toCrearExcepcion" uri="direct:excepcion"/>
                </doCatch>
            </doTry>
        </route>
        <route id="ValidarCamposEntrada">
            <from id="ValidarCamposEntrada-From" uri="direct:validarCamposEntrada"/>
            <doTry id="ValidarCamposEntrada-DoTry">
                <to id="ValidarCamposEntrada-SQL" uri="sql-stored:classpath:etc/sql/pa_obtenerCamposEntrada.sql?dataSource=dataSourceSqlServer&amp;outputHeader=responseCamposEntrada"/>
                <bean id="ValidarCamposEntrada-Bean"
                    method="validarCamposEntrada" ref="beanRouteFacade"/>
                <choice id="ValidaCanal-Choice">
                    <when id="ValidaCanal-When">
                        <simple>${exchangeProperty.MensajeSalida} != 'OK'</simple>
                        <removeHeaders id="ValidaCanal-RemoveHeaders" pattern="*"/>
                        <setProperty id="ValidaCanal-CONTENT_TYPE" propertyName="Exchange.CONTENT_TYPE">
                            <constant>text/xml;charset=utf-8</constant>
                        </setProperty>
                        <setProperty
                            id="ValidaCanal-HTTP_CHARACTER_ENCODING" propertyName="Exchange.HTTP_CHARACTER_ENCODING">
                            <constant>utf-8</constant>
                        </setProperty>
                        <setProperty id="ValidaCanal-CONTENT_ENCODING" propertyName="Exchange.CONTENT_ENCODING">
                            <constant>utf-8</constant>
                        </setProperty>
                        <setProperty id="ValidaCanal-CHARSET_NAME" propertyName="Exchange.CHARSET_NAME">
                            <constant>utf-8</constant>
                        </setProperty>
                        <stop id="ErrorValidacionCanal"/>
                    </when>
                </choice>
                <doCatch id="OrigenDatosNotificacion-DoCatch">
                    <exception>java.lang.Exception</exception>
                    <setProperty
                        id="OrigenDatosNotificacion-SetProperty" propertyName="codigoError">
                        <simple>{{error.base.datos.codigo.notificacion}}</simple>
                    </setProperty>
                    <setProperty
                        id="OrigenDatosNotificacion-SetProperty-MensajeSalida" propertyName="MensajeSalida">
                        <simple>{{error.base.datos.descripcion.notificacion}}</simple>
                    </setProperty>
                    <setProperty
                        id="OrigenDatosNotificacion-SetProperty-BaseDatos" propertyName="BaseDatos">
                        <simple>{{error.base.datos.notificacion}}</simple>
                    </setProperty>
                    <to id="OrigenDatosNotificacion-To-Excepcion" uri="direct:excepcion"/>
                    <removeHeaders
                        id="OrigenDatosNotificacion-RemoveHeaders" pattern="*"/>
                    <setProperty
                        id="OrigenDatosNotificacion-CONTENT_TYPE" propertyName="Exchange.CONTENT_TYPE">
                        <constant>text/xml;charset=utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="OrigenDatosNotificacion-HTTP_CHARACTER_ENCODING" propertyName="Exchange.HTTP_CHARACTER_ENCODING">
                        <constant>utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="OrigenDatosNotificacion-CONTENT_ENCODING" propertyName="Exchange.CONTENT_ENCODING">
                        <constant>utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="OrigenDatosNotificacion-CHARSET_NAME" propertyName="Exchange.CHARSET_NAME">
                        <constant>utf-8</constant>
                    </setProperty>
                    <stop id="OrigenDatosNotificacion-Stop"/>
                </doCatch>
            </doTry>
        </route>
        <route id="LLamadaHabilitadoSolicitud">
            <from id="ObtenerHabilitadoSolicitud-From" uri="direct:pa_consultarHabilitadosSolicitud"/>
            <doTry id="ObtenerHabilitadoSolicitud-Try">
                <setProperty id="vobo" propertyName="moduleName">
                    <simple>{{nombre.modulo.portal.convenio}}</simple>
                </setProperty>
                <to id="ObtenerHabilitadoSolicitud-SQL" uri="sql-stored:classpath:etc/sql/pa_consultarHabilitadosSolicitud.sql?dataSource=dataSourcePortalConvenio&amp;outputHeader=responseHabilitadoSolicitud"/>
                <bean id="ObtenerHabilitadoSolicitud-Bean"
                    method="obtenerHabilitadoSolicitud" ref="beanRouteFacade"/>
                <doCatch id="ObtenerHabilitadoSolicitud-DoCatch">
                    <exception>java.lang.Exception</exception>
                    <setProperty
                        id="ObtenerHabilitadoSolicitud-SetProperty" propertyName="codigoError">
                        <simple>{{error.base.datos.codigo.convenio}}</simple>
                    </setProperty>
                    <setProperty
                        id="ObtenerHabilitadoSolicitud-SetProperty-MensajeSalida" propertyName="MensajeSalida">
                        <simple>{{error.base.datos.descripcion.convenio}}</simple>
                    </setProperty>
                    <setProperty
                        id="ObtenerHabilitadoSolicitud-SetProperty-BaseDatos" propertyName="BaseDatos">
                        <simple>{{error.base.datos.convenio}}</simple>
                    </setProperty>
                    <to id="ObtenerHabilitadoSolicitud-To-Excepcion" uri="direct:excepcion"/>
                    <removeHeaders
                        id="ObtenerHabilitadoSolicitud-RemoveHeaders" pattern="*"/>
                    <setProperty
                        id="ObtenerHabilitadoSolicitud-CONTENT_TYPE" propertyName="Exchange.CONTENT_TYPE">
                        <constant>text/xml;charset=utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="ObtenerHabilitadoSolicitud-HTTP_CHARACTER_ENCODING" propertyName="Exchange.HTTP_CHARACTER_ENCODING">
                        <constant>utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="ObtenerHabilitadoSolicitud-CONTENT_ENCODING" propertyName="Exchange.CONTENT_ENCODING">
                        <constant>utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="ObtenerHabilitadoSolicitud-CHARSET_NAME" propertyName="Exchange.CHARSET_NAME">
                        <constant>utf-8</constant>
                    </setProperty>
                    <stop id="ObtenerHabilitadoSolicitud-Stop"/>
                </doCatch>
            </doTry>
        </route>
        <route id="WSEjecutivo">
            <from id="ObtenerDatosECC-From" uri="direct:ObtenerDatosECC"/>
            <doTry id="ObtenerDatosECC-Try">
                <removeHeaders id="ObtenerDatosECC-removeHeaders" pattern="*"/>
                <setHeader headerName="operationName" id="ObtenerDatosECC-OperationName">
                    <constant>consultarEEC</constant>
                </setHeader>
                <setHeader headerName="SOAPAction" id="ObtenerDatosECC-SOAPAction">
                    <constant>http://integracion.coopeuch.cl/wsejecutivo/consultarEEC</constant>
                </setHeader>
                <setHeader headerName="operationNamespace" id="ObtenerDatosECC-OperationNamespace">
                    <constant>http://integracion.coopeuch.cl/wsejecutivo</constant>
                </setHeader>
                <bean id="ObtenerDatosECC-Request-Bean"
                    method="requestConsultarECC" ref="beanRouteFacade"/>
                <to id="ObtenerDatosECC-To-CXF" uri="cxf:bean:endpointWSEjecutivo"/>
                <bean id="ObtenerDatosECC-Response-Bean"
                    method="responseConsultarECC" ref="beanRouteFacade"/>
                <doCatch id="ObtenerDatosECC-DoCatch">
                    <exception>java.lang.Exception</exception>
                    <setProperty id="OObtenerDatosECC-SetProperty" propertyName="codigoError">
                        <simple>{{error.servicio.wsejecutivo.codigo}}</simple>
                    </setProperty>
                    <setProperty
                        id="ObtenerDatosECC-SetProperty-MensajeSalida" propertyName="MensajeSalida">
                        <simple>{{error.servicio.wsejecutivo.descripcion}}</simple>
                    </setProperty>
                    <setProperty
                        id="ObtenerDatosECC-SetProperty-BaseDatos" propertyName="BaseDatos">
                        <simple>{{error.servicio.wsejecutivo}}</simple>
                    </setProperty>
                    <to id="ObtenerDatosECC-To-Excepcion" uri="direct:excepcion"/>
                    <removeHeaders id="ObtenerDatosECC-RemoveHeaders" pattern="*"/>
                    <setProperty id="ObtenerDatosECC-CONTENT_TYPE" propertyName="Exchange.CONTENT_TYPE">
                        <constant>text/xml;charset=utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="ObtenerDatosECC-HTTP_CHARACTER_ENCODING" propertyName="Exchange.HTTP_CHARACTER_ENCODING">
                        <constant>utf-8</constant>
                    </setProperty>
                    <setProperty id="ObtenerDatosECC-CONTENT_ENCODING" propertyName="Exchange.CONTENT_ENCODING">
                        <constant>utf-8</constant>
                    </setProperty>
                    <setProperty id="ObtenerDatosECC-CHARSET_NAME" propertyName="Exchange.CHARSET_NAME">
                        <constant>utf-8</constant>
                    </setProperty>
                    <stop id="ObtenerDatosECC-Stop"/>
                </doCatch>
            </doTry>
        </route>
        <route id="WSSucursal">
            <from id="ObtenerDatosPersonalSucursal-From" uri="direct:ObtenerDatosPersonalSucursal"/>
            <doTry id="ObtenerDatosPersonalSucursal-Try">
                <removeHeaders
                    id="ObtenerDatosPersonalSucursal-removeHeaders" pattern="*"/>
                <setHeader headerName="operationName" id="ObtenerDatosPersonalSucursal-OperationName">
                    <constant>consultarFuncionario</constant>
                </setHeader>
                <setHeader headerName="SOAPAction" id="ObtenerDatosPersonalSucursal-SOAPAction">
                    <constant>http://integracion.coopeuch.cl/WSSucursal/consultarFuncionario</constant>
                </setHeader>
                <setHeader headerName="operationNamespace" id="ObtenerDatosPersonalSucursal-OperationNamespace">
                    <constant>http://integracion.coopeuch.cl/WSSucursal/</constant>
                </setHeader>
                <bean id="ObtenerDatosPersonalSucursal-Request-Bean"
                    method="requestConsultarFuncionario" ref="beanRouteFacade"/>
                <to id="ObtenerDatosPersonalSucursal-To-CXF" uri="cxf:bean:endpointWSSucursal"/>
                <bean id="ObtenerDatosPersonalSucursal-Response-Bean"
                    method="responseConsultarFuncionario" ref="beanRouteFacade"/>
                <doCatch id="ObtenerDatosPersonalSucursal-DoCatch">
                    <exception>java.lang.Exception</exception>
                    <setProperty
                        id="ObtenerDatosPersonalSucursal-SetProperty" propertyName="codigoError">
                        <simple>{{error.servicio.wssucursal.codigo}}</simple>
                    </setProperty>
                    <setProperty
                        id="ObtenerDatosPersonalSucursal-SetProperty-MensajeSalida" propertyName="MensajeSalida">
                        <simple>{{error.servicio.wssucursal.descripcion}}</simple>
                    </setProperty>
                    <setProperty
                        id="ObtenerDatosPersonalSucursal-SetProperty-BaseDatos" propertyName="BaseDatos">
                        <simple>{{error.servicio.wssucursal}}</simple>
                    </setProperty>
                    <to id="ObtenerDatosPersonalSucursal-To-Excepcion" uri="direct:excepcion"/>
                    <removeHeaders
                        id="ObtenerDatosPersonalSucursal-RemoveHeaders" pattern="*"/>
                    <setProperty
                        id="ObtenerDatosPersonalSucursal-CONTENT_TYPE" propertyName="Exchange.CONTENT_TYPE">
                        <constant>text/xml;charset=utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="ObtenerDatosPersonalSucursal-HTTP_CHARACTER_ENCODING" propertyName="Exchange.HTTP_CHARACTER_ENCODING">
                        <constant>utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="ObtenerDatosPersonalSucursal-CONTENT_ENCODING" propertyName="Exchange.CONTENT_ENCODING">
                        <constant>utf-8</constant>
                    </setProperty>
                    <setProperty
                        id="ObtenerDatosPersonalSucursal-CHARSET_NAME" propertyName="Exchange.CHARSET_NAME">
                        <constant>utf-8</constant>
                    </setProperty>
                    <stop id="ObtenerDatosPersonalSucursal-Stop"/>
                </doCatch>
            </doTry>
        </route>
        <route id="excepcion">
            <from id="excepcionfrom" uri="direct:excepcion"/>
            <doTry id="excepcionTry">
                <bean id="beanExceptionAmq" method="excepcionAmq" ref="beanRouteFacade"/>
                <marshal id="excepcionMarshalJson">
                    <json library="Jackson"/>
                </marshal>
                <convertBodyTo id="excepcionConvertTo" type="java.lang.String"/>
                <to id="excepcionToAmqFault" uri="beanAMQ:{{amq.queue.fault}}?errorHandlerLoggingLevel=TRACE&amp;exchangePattern=InOnly"/>
                <bean id="excepcionBeanExcepcion"
                    method="excepcionDestinatario" ref="beanRouteFacade"/>
                <doCatch id="excepcionDoCatch">
                    <exception>java.lang.Exception</exception>
                    <setBody id="exceptionFinalSetBodyError">
                        <simple>${exception}, Body -&gt; ${body}</simple>
                    </setBody>
                    <log id="excepcionLog" loggingLevel="ERROR" message="Ha ocurrido un error al escribir en la cola de errores o en la ejecucion del servicio: ${body}"/>
                </doCatch>
            </doTry>
        </route>
    </camelContext>
</blueprint>
