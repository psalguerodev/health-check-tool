                <bean id="crearBean" method="validarIngreso" ref="beanRouteFacade"/>
                <to id="spConsultarDatos" uri="sql-stored:classpath:etc/sql/PA_SEL_MODELOATENCION_CREAR_WS.sql?outputHeader=ConsultarPaResponse&amp;dataSource=dataSourceSqlDb2"/>
