                    <to id="spinsDatosCrm" uri="sql-stored:classpath:etc/sql/PA_INSDatosCrm.sql?outputHeader=INScrmPaResponse&amp;dataSource=dataSourceSqlServer"/>
                </split>
                <doCatch id="selectDatosCRmDoCatchEspecifico">
                    <exception>java.lang.Exception</exception>
                    <setProperty
