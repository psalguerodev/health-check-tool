                        <bean id="responseBeanUpdateTempus"
                            method="responseUpdateTempus" ref="beanRouteFacade"/>
                    </when>
                    <when id="_when4">
                        <simple>${header.operationName} == 'cargamasiva'</simple>
                        <bean id="responseBeanUpdatemasivoTempus"
                            method="responseUpdatemasivoTempus" ref="beanRouteFacade"/>
                    </when>
                </choice>
                <removeHeaders id="_removeHeaders" pattern="*"/>
                <doCatch id="updateDatosTempusDoCatchEspecifico">
                    <exception>java.lang.IllegalArgumentException</exception>
                    <setProperty
                        id="updateDatosTempusCodigoErrorEspecifico" propertyName="codigoError">
