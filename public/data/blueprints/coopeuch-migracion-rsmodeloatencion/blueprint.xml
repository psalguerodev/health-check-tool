        <property name="minEvictableIdleTimeMillis" value="[{jdbc.sqlserver.minEvictableIdleTimeMillis}]"/>
        <property name="maxActive" value="[{jdbc.sqlserver.maxActive}]"/>
    </bean>
    <bean class="org.apache.commons.dbcp.BasicDataSource"
